#include <iostream>
#include <conio.h>
using namespace std;

int main() {

    int filas=3,columnas=3;

    int matriz[3][3];
    for (int i=0; i<filas; i++){
        for (int j = 0; j < columnas; j++){
            cout<<"Ingrese el dato de la posicion "<<i+1<<","<<j+1<<": ";
            cin>>matriz[i][j];
        }
    }

    for (int k=0; k<columnas; k++) {
        cout<<k+1<<","<<k+1<<": "<< matriz[k][}]<<endl;
    }

    getch();
    return 0;
}
